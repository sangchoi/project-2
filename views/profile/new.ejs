<h2>CREATE NEW PROFILE</h2>

<form action="/profile" method="POST">
    
    <div class="form-group">
      <label for="birthYear">Birth Year</label>
      <input id="birthYear" class="form-control" type="number" name="birthYear">
    </div>
  
    <div class="form-group">
      <label for="gender">Gender</label>
      <input id="gender" class="form-control" type="text" name="gender">
    </div>
  
    <div class="form-group">
      <label for="city">City</label>
      <input id="city" class="form-control" type="text" name="city">
    </div>
  
    <div class="form-group">
      <label for="state">State</label>
      <input id="state" class="form-control" type="text" name="state">
    </div>
  
    <div class="form-group">
      <label for="country">Country</label>
      <input id="country" class="form-control" type="text" name="country">
    </div>
  
    <div class="form-group">
      <label for="language">Language</label>
      <input id="language" class="form-control" type="text" name="language">
    </div>
  
    <input class="btn btn-primary" type="submit">
  
  
  
  
    
  </form>
